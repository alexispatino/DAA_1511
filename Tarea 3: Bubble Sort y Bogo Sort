      "source": [
        "import time\n",
        "inicio = time.time()\n",
        "\n",
        "def burbuja( data):\n",
        "    n = len(data)\n",
        "    for iteracion in range( n ):\n",
        "        for pivote in range( n - iteracion - 1):\n",
        "            if data[pivote] > data[pivote +1]:\n",
        "                tmp = data[pivote]\n",
        "                data[pivote]=data[pivote+1]\n",
        "                data[pivote+1]=tmp\n",
        "    #print(data)\n",
        "\n",
        "import numpy as np\n",
        "original = np.random.randint(1,200000000, size=100000)\n",
        "\n",
        "burbuja( original)\n",
        "\n",
        "fin = time.time()\n",
        "print(fin-inicio)"
      ]
