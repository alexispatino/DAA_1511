      "source": [
        "import time\n",
        "inicio = time.time()\n",
        "\n",
        "import random\n",
        "def bogo( data ):\n",
        "    ordenado = False\n",
        "    #print(data)\n",
        "    while not ordenado:\n",
        "        random.shuffle(data)\n",
        "        for pivote in range( len(data)-1):\n",
        "            if data[pivote] <=data[pivote+1]:\n",
        "                ordenado = True\n",
        "            else:\n",
        "                ordenado = False\n",
        "                break\n",
        "    print(\"Final\",data)\n",
        "\n",
        "\n",
        "original = [93,104,9,43,54,123,323,8945,542,911,4564,123466]\n",
        "\n",
        "#\n",
        "bogo(original)\n",
        "\n",
        "fin = time.time()\n",
        "print(fin-inicio)\n"
      ]
    }
